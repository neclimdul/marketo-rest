{
    "name": "neclimdul/marketo-rest",
    "description": "Marketo Rest API library",
    "license": "MIT",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "authors": [
        {
            "name": "James Gilliland",
            "email": "neclimdul@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 | ^7.0",
        "neclimdul/oauth2-marketo": "^2.0",
        "softonic/guzzle-oauth2-middleware": "^1.3 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "~3.0",
        "illuminate/support": "^7.11",
        "phpspec/prophecy-phpunit": "^2.0",
        "vimeo/psalm": "^4.20",
        "phpstan/phpstan": "^1.4",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "jangregor/phpstan-prophecy": "^1.0"
    },
    "autoload": {
        "psr-4": {"NecLimDul\\MarketoRest\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"NecLimDul\\MarketoRest\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "NecLimDul\\MarketoRest\\Laravel\\MarketoRestProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs",
        "static": [
            "parallel-lint src tests",
            "phpstan",
            "psalm src"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
