includes:
	- phpstan-baseline.neon
parameters:
	level: 7
	treatPhpDocTypesAsCertain: true
	checkGenericClassInNonGenericObjectType: false
	paths:
		- src
		- tests
	ignoreErrors:
		- '#Method .*\\ObjectSerializer::sanitizeForSerialization\(\) return type has no value type specified in iterable type array\.$#'
